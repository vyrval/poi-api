{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/extensions": ["error", "ignorePackages"]
    }
}
